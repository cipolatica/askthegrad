

<div class='container-fluid animate-fade-in-500'>
  <div class="row">
    <div class="col-lg-12">
      <div class="panel-group" id="accordion">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h4 class="panel-title"> <!-- DONT FORGET: the href from dressed had  href="006_components@fa-briefcase%252Faccordian.html#collapseOne" YOU may need to add full URL -->
              <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                Highest Rated Colleges <small><em>overall</em></small>
              </a>
            </h4>
          </div>
          <div id="collapseOne" class="panel-collapse collapse">
            <div class="panel-body" style="padding: 0 15px 15px 15px;">
              <div class="table-responsive">
                <table class="table table-striped" style="margin-bottom: 0;">
                  <tbody>
                  <% names_array = @stats.top_college_rating_names.to_s.split("^")%>
                  <% ids_array = @stats.top_college_rating_ids.to_s.split("^")%>
                  <% amounts_array = @stats.top_college_rating_amounts.to_s.split("^")%>
                  <% (0..names_array.length-1).each do |index|%>
                      <tr>
                        <td><%= index + 1 %></td>
                        <td>
                          <ul class="list-unstyled small" style="margin-bottom: 0;">
                            <li style="margin-bottom: 0;">
                              <% if amounts_array[index] != nil && amounts_array[index].to_s != "" %>
                                  <b><%= link_to names_array[index].to_s, school_reviews_path(:the_school => { :school_id => ids_array[index].to_i })  %></b><br/>
                                 <strong><%= number_with_precision(amounts_array[index].to_f,precision:1) %></strong> <small><em>out of 5</em></small>
                              <% else %>
                                  <b>N/A</b>
                              <% end %>
                            </li>
                          </ul>
                        </td>
                      </tr>
                  <% end %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
